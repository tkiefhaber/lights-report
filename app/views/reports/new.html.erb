<div class='box box-nomargin'>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: name %>
  <% end %>

  <div class='box-content'>

    <div class='text-center'>
      <%= form_for @report do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name, placeholder: 'example: Maplewood Sept 2014', size: 75 %>
        <%= f.file_field :report, class: 'file-input-wrapper btn' %>
        <%= f.submit 'Upload', :class => 'btn btn-success btn-medium' %>
      <% end %>
      <br/>
    </div>
  </div>
</div>

